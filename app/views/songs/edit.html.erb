<h1>Edit Song Form</h1>
<%= form_for(@song) do |f| %>
<%= label_tag(:name, "Song Name:") %><br>
<%= f.text_field :name  %><br>
<%= label_tag(:artist_id, "Song Artist:") %><br>
<%= f.text_field :artist_id  %><br>
<%= label_tag(:genre_id, "Song Genre:") %><br>
<%= f.text_field :genre_id  %><br>

<%= f.submit  %>
<% end %>